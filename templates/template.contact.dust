<div class="contact-container">
   <div class="avatar-img-wrapper">
      <img class="avatar-img" src="http://placehold.it/200x200" alt="">
   </div>
   <h3>
      {name}
   </h3>
   <p>Phone: {phone}</p>
   <p>Group: {group}</p>
   <div class="contact-btn-wrapper">
      <a href="#contacts/edit/{id}" class="edit-contact btn btn-info"><span></span> Edit</a>
      <a href="#contacts/delete/{id}" class="delete-contact btn btn-danger">
      <span></span> Delete
      </a>
   </div>
</div>